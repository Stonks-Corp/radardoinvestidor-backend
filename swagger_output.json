    "/api/pesquisa": {
      "get": {
        "tags": [
          "Rotas"
        ],
        "summary": "Pesquisa uma string no banco de dados",
        "description": "Retorna os fundos encontrados no banco de dados.\nAtualmente, busca pelas colunas de denominação social do fundo e pelo cnpj do fundo.",
        "operationId": "pesquisa",
        "parameters": [
          {
            "name": "s",
            "in": "query",
            "description": "Recebe uma palavra ou segmento do qual deseja realizar a pesquisa no banco de dados.",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "400": {
            "description": "Bad Request"
          }
        }
      }
    },
    "/api/fundo": {
      "post": {
        "tags": [
          "Rotas"
        ],
        "summary": "Adiciona um novo fundo no banco de dados.",
